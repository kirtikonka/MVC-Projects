@if (TempData["Updmsg"]!=null)
{
    <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script>
        toastr.success('@TempData["Updmsg"]')
    </script>
}
@if (TempData["Delmsg"] != null)
{
    <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script>
        toastr.error('@TempData["Delmsg"]')
    </script>
}
